<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="458.0" prefWidth="822.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.co_po_assessment.ManageCourseAssignmentsController">
    <children>
        <TableView fx:id="courseTableView" layoutX="-1.0" layoutY="70.0" prefHeight="318.0" prefWidth="822.0">
            <columns>
                <TableColumn fx:id="courseCodeColumn" prefWidth="117.0" text="Course Code" />
                <TableColumn fx:id="courseNameColumn" prefWidth="268.0" text="Course Name" />
                <TableColumn fx:id="facultyColumn" prefWidth="281.0" text="Faculty" />
                <TableColumn fx:id="academicYearColumn" prefWidth="155.0" text="Academic Year" />
            </columns>
        </TableView>
        <Button fx:id="assignCourseButton" layoutX="230.0" layoutY="411.0" mnemonicParsing="false" onAction="#onAssignCourseButton" text="Assign Course" />
        <Button fx:id="removeCourseButton" layoutX="354.0" layoutY="411.0" mnemonicParsing="false" onAction="#onRemoveCourseButton" text="Remove Course" />
        <Button fx:id="backButton" layoutX="489.0" layoutY="411.0" mnemonicParsing="false" onAction="#onBackButton" text="Go Back" />
    </children>
</AnchorPane>
