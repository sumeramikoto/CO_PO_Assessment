<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="458.0" prefWidth="685.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.co_po_assessment.admin_input_controller.ManageFacultiesController">
   <children>
      <TableView fx:id="facultyTableView" layoutX="-1.0" layoutY="70.0" prefHeight="318.0" prefWidth="685.0">
        <columns>
          <TableColumn fx:id="idColumn" prefWidth="90.0" text="ID" />
          <TableColumn fx:id="nameColumn" prefWidth="209.0" text="Name" />
            <TableColumn fx:id="shortnameColumn" prefWidth="114.0" text="Shortname" />
            <TableColumn fx:id="emailColumn" prefWidth="271.0" text="Email" />
        </columns>
      </TableView>
      <Button fx:id="addFacultyButton" layoutX="117.0" layoutY="411.0" mnemonicParsing="false" onAction="#onAddFacultyButton" text="Add Faculty" />
      <Button fx:id="removeFacultyButton" layoutX="286.0" layoutY="411.0" mnemonicParsing="false" onAction="#onRemoveFacultyButton" text="Remove Faculty" />
      <Button fx:id="backButton" layoutX="487.0" layoutY="411.0" mnemonicParsing="false" onAction="#onBackButton" text="Go Back" />
   </children>
</AnchorPane>
